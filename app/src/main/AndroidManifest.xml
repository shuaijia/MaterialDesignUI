<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jia.materialdesignui">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ButtonActivity"
            android:label="Button" />
        <activity
            android:name=".SwitchActivity"
            android:label="Switch" />
        <activity
            android:name=".ProgressActivity"
            android:label="Progress" />
        <activity
            android:name=".SnackBarActivity"
            android:label="SnackBar" />
        <activity
            android:name=".DialogActivity"
            android:label="Dialog" />
        <activity
            android:name=".ColorSelectorActivity"
            android:label="ColorSelector" />
    </application>

</manifest>